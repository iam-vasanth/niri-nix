layout {

    focus-ring {
        active-color   "#ffb4aa"
        inactive-color "#1a1110"
        urgent-color   "#ffb4ab"
    }

    border {
        active-color   "#ffb4aa"
        inactive-color "#1a1110"
        urgent-color   "#ffb4ab"
    }

    shadow {
        color "#00000070"
    }

    tab-indicator {
        active-color   "#ffb4aa"
        inactive-color "#73342d"
        urgent-color   "#ffb4ab"
    }

    insert-hint {
        color "#ffb4aa80"
    }
}

recent-windows {
    highlight {
        active-color "#ffb4aa"
        urgent-color "#ffb4ab"
    }
}
